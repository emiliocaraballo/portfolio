  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development PORT=3000 ts-node-dev src/index.ts",
    "build": "tsc",
    "start": "NODE_ENV=production PORT=3000 node dist/index.js",
    "strategy": "NODE_ENV=strategy PORT=3000 node dist/index.js",
    "bd:update": "knex --knexfile src/database/knexfile.ts migrate:latest",
    "bd:rollback": "knex --knexfile src/database/knexfile.ts migrate:rollback",
    "bd:create": "knex --knexfile src/database/knexfile.ts migrate:make -x ts --",
    "data:create": "knex --knexfile src/database/knexfile.ts seed:make -x ts",
    "data:update": "knex --knexfile src/database/knexfile.ts seed:run"
  },
